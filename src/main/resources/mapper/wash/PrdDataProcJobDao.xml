<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.dfjinxin.modules.wash.dao.PrdDataProcJobDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.dfjinxin.modules.wash.entity.PrdDataProcJobEntity" id="prdDataProcJobMap">
        <result property="jobid" column="Jobid"/>
        <result property="jobType" column="Job_Type"/>
        <result property="dataTblid" column="Data_Tblid"/>
        <result property="jobReleaseDate" column="Job_Release_Date"/>
        <result property="jobExecStatus" column="Job_Exec_Status"/>
        <result property="jobStartTime" column="Job_Start_Time"/>
        <result property="jobFinishTime" column="Job_Finish_Time"/>
        <result property="jobExpireDate" column="Job_Expire_Date"/>
        <result property="jobScheduleType" column="Job_Schedule_Type"/>
    </resultMap>


    <update id="updateJobById" parameterType="io.dfjinxin.modules.wash.entity.PrdDataProcJobEntity">
        update prd_data_proc_job
        set
        Job_Type=#{jobType},
        Data_Tblid=#{dataTblid},
        Job_Release_Date=#{jobReleaseDate},
        Job_Exec_Status=#{jobExecStatus},
        Job_Start_Time=#{jobStartTime},
        Job_Finish_Time=#{jobFinishTime},
        Job_Expire_Date=#{jobExpireDate}
         where Jobid=#{jobid}
    </update>

</mapper>